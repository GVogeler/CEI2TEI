                <cei:chDesc>
                    <cei:abstract>Vertrag zwischen der Jean de Bourgogne, Bischof von Cambrai und
                        der Stadt auf der einen Seite und den Kapitel von Cambrai auf der anderen
                        Seite.</cei:abstract>
                    <cei:issued>
                        <cei:placeName/>
                        <cei:dateRange from="14460609" to="14460609">1446-06-09</cei:dateRange>
                    </cei:issued>
