                                                  <cei:chDesc>
                                                  <cei:abstract>Francesco Gonzaga gewährt dem
                                                  Cristoforo Poggi Freiheiten in Bezug aus dessen
                                                  Villa „Corte Villagrossa“. </cei:abstract>
                                                  <cei:issued>
                                                  <cei:placeName>Mantua</cei:placeName>
