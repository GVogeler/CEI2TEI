                <cei:chDesc>
                    <cei:abstract>Kaiser Heinrich [IV.] bestätigt dem Kloster Santa Maria in Pomposa
                        seinen Schutz, die Reichsunmittelbarkeit, das Wahlrecht und den
                        Besitzstand.<cei:lb/></cei:abstract>
                    <cei:issued>
                        <cei:placeName>Garda</cei:placeName>
                        <cei:dateRange from="10951007" to="10951007">1095-10-07</cei:dateRange>
