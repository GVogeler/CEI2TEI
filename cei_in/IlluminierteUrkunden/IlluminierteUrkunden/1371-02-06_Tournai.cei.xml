                <cei:chDesc>
                    <cei:abstract>Karl V. bestätigt das Stadtrecht von Tournai </cei:abstract>
                    <cei:issued>
                        <cei:placeName/>
                        <cei:dateRange from="13710206" to="13710206">1371-02-06</cei:dateRange>
