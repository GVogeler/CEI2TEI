                <cei:chDesc>
                    <cei:abstract>Ablass von Kardinal Zbigniew Olśniecki für die Allerheiligen
                        Kirche in Krakau </cei:abstract>
                    <cei:issued>
                        <cei:placeName>Krakau</cei:placeName>
                        <cei:dateRange from="14491023" to="14491023">1449-10-23</cei:dateRange>
                    </cei:issued>
                    <cei:witnessOrig>
                        <cei:traditioForm>Original</cei:traditioForm>
                        <cei:figure>
                            <cei:graphic url="http://images.monasterium.net/illum/IllUrk/1449-10-23_Zbigniew-Olesnicki-Bi-v-Krakau-Kardinal-fuer-Allerheiligenkirche-in-Krakau_Europa-Jagellonica-2013-11.jpg"/>
                        </cei:figure>
                        <cei:archIdentifier><cei:settlement>Krakau</cei:settlement><cei:arch>Kapitelarchiv</cei:arch><cei:idno>sign. 430</cei:idno></cei:archIdentifier>
                        <cei:physicalDesc>
                            <cei:decoDesc>
                                <cei:p>Zu Beginn der Urkunde ein grosses Bildfeld mit thronendem
                                    Papst von zwei Kardinälen flankiert. Darüber ein schmäleres
                                    Bildfeld mit den thronenden Christus von allen heiligen umgeben;
                                    dieses Bildfeld von zwei knienden Engeln gehalten; links des
                                    Schriftspiegels eine Ranke, die die Herkunft aus dem Umfeld der
                                    Martyriologium-Werkstatt noch deutlich zeigt.</cei:p>
