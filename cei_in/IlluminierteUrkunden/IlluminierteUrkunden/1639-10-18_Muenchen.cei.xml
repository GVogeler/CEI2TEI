        <cei:chDesc>
          <cei:abstract>[Herzog Maximilian I. von Bayern,] <cei:index indexName="illurk-vocabulary"
              lemma="">gedrucktes </cei:index><cei:index indexName="Illurk-Urkundenart"
              >Münzmandat</cei:index>(Münzverruf), Fragment eines <cei:lb/>Rahel
            Ackermann</cei:abstract>
          <cei:issued>
            <cei:placeName>München (Druck: s.l. [ohne Ortsangabe], s.n. [kein Drucker genannt bzw.
              festgestellt])</cei:placeName>
            <cei:dateRange from="16391018" to="16391018">1639-10-18 (Druck: s. d. [ohne
              Datum])</cei:dateRange>
          </cei:issued>
