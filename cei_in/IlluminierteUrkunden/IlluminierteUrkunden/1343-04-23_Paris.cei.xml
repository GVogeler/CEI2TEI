                <cei:idno id="1343-04-23_Paris">1343-04-23_Paris</cei:idno>
                <cei:chDesc>
                    <cei:abstract>Vertrag zwischen Philippe de Valois und Humbert II., Dauphin der
                        Vienne, über die Abtretung der Dauphiné. </cei:abstract>
                    <cei:issued>
                        <cei:placeName>Bois de Vincennes</cei:placeName>
                        <cei:dateRange from="13430423" to="13430423">1343-04-23</cei:dateRange>
                    </cei:issued>
                    <cei:witnessOrig>
                        <cei:traditioForm>Original</cei:traditioForm>
                        <cei:figure>
                            <cei:graphic url="http://images.monasterium.net/illum/IllUrk/1343-04-23_Philipp_VI_und_Humbert_II_Dauphin_der_Wiener_AE_II_352_Detail.jpeg"/>
                        </cei:figure>
                        <cei:archIdentifier>
                            <cei:settlement>Paris</cei:settlement>
                            <cei:arch>Archives nationales</cei:arch>
                            <cei:idno>AE//II/352</cei:idno>
                            <cei:altIdentifier type="old">
                                <cei:idno>J 279, no 4</cei:idno>
                            </cei:altIdentifier>
                        </cei:archIdentifier>
                        <cei:physicalDesc>
                            <cei:decoDesc>
                                <cei:p n="Ekphrasis">Die tintenfarbigen Initialen „Ph“ mit
                                    ausgespartem Flechtbandornament; aus der Spitze des „P“ eine
                                    Fleur-de-lys heraustretend, der Schaft des „h“ bekrönt von einer
                                    Krone. In dunklerem Braun als der Urkundentext
                                    gezeichnet.</cei:p>
                                <cei:p n="Stil und Einordnung">Nach Jeay (Images du pouvoir, 2013,
                                    S. 60) ist die Fleur-de-lys ebenso wie die Krone die angemessene
                                    Dekoration für diesen Vertrag. In der Verbindung mit der
                                    Initiale geben sie ein symbolisches Bild des französischen
                                    Königs.</cei:p>
                                <cei:p n="Autorensigle">Gabriele Bartz</cei:p>
                            </cei:decoDesc>
