  skos:narrower annot:endorsement;
  skos:narrower annot:call_number;
  skos:definition "TBD"@en.

annot:commercial rdf:type skos:Concept; 
  skos:prefLabel "annotation related to sale or price"@en;
  skos:definition "TBD"@en.

